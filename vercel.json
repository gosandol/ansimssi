{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/*.py": {
            "maxDuration": 10
        }
    },
    "regions": [
        "icn1"
    ]
}